    "email": "email dev --dir ./src/infrastructure/email/templates"
  },
  "devDependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@types/bun": "^1.2.5",
    "@types/react": "^19.0.10",
    "drizzle-kit": "^0.30.5",
    "react-email": "^3.0.7"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/jwt": "^1.2.0",
    "@elysiajs/swagger": "^1.2.2",
    "@react-email/components": "^0.0.33",
    "drizzle-orm": "^0.40.0",
    "elysia": "^1.2.25",
    "firebase-admin": "^13.3.0",
    "mysql2": "^3.14.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redis": "^4.7.0",
    "resend": "^4.1.2"
